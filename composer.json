{
    "name": "oauth-server",
    "description": "Sample Oauth Server built up from various web tutorials. Please don't use in the wild.",
    "type": "project",
    "keywords": [
        "slim",
        "oauth",
        "api"
    ],
    "authors": [
        {
            "name": "Dave Gillard",
            "email": "dgl_home@hotmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "bshaffer/oauth2-server-php": "^1.10",
        "illuminate/database": "~5.1",
        "monolog/monolog": "^1.17",
        "rakit/validation": "^1.1",
        "robmorgan/phinx": "^0.9.2",
        "slim/php-view": "^2.2",
        "slim/slim": "^3.11",
        "tuupola/slim-basic-auth": "^3.2",
        "vlucas/phpdotenv": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "OAuthServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAuthServer\\": "src/"
        }
    },
    "require-dev": {
        "overtrue/phplint": "^1.0",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
